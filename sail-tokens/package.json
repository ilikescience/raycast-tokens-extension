{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "sail-tokens",
  "title": "Sail Tokens",
  "description": "Helps you use sail tokens",
  "icon": "command-icon.png",
  "author": "matt_strom-awn",
  "categories": [
    "Design Tools",
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Show Sail Tokens",
      "subtitle": "Search and copy tokens",
      "description": "Search and filter Sail's design tokens and copy them in varioius formats",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.64.2",
    "colorjs.io": "^0.4.5",
    "js-beautify": "^1.14.11"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/js-beautify": "^1.14.3",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
